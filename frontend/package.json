{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:frontend": "npm run dev",
    "dev:backend": "cd ../backend && \".venv/Scripts/activate.bat\" && python run.py",
    "prod:backend": "cd ../backend && python run.py",
    "dev:all": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "prod:all": "concurrently \"npm:start\" \"npm:prod:backend\"",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^15.2.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-webcam": "^7.2.0",
    "concurrently": "^9.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3",
    "typescript": "^5"
  }
}
